{
  "name": "express_sequelize",
  "version": "1.0.0",
  "description": "NodeJs/TypeScript test",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "tsc && sequelize-cli db:migrate",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "nodemonConfig": {
    "delay": 1000,
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/init.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^2.29.3",
    "express": "^4.18.1",
    "sequelize": "^6.23.1",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.1.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "ts-loader": "^9.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/supertest": "^2.0.12",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbeetechnologies/node_test_new.git"
  },
  "bugs": {
    "url": "https://github.com/webbeetechnologies/node_test_new/issues"
  },
  "homepage": "https://github.com/webbeetechnologies/node_test_new#readme"
}
